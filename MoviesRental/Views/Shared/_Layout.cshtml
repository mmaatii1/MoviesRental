<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MoviesRental</title>

    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap (2).css" />
    <link href="~/datatables/css/dataTables.bootstrap4.css" rel="stylesheet" />
    <link href="~/css/typeahead.css" rel="stylesheet" />
    <link href="~/toastr.js/toastr.css" rel="stylesheet" />
</head>
<body>
    @Html.Partial("_NavBar")
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - MoviesRental - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/toastr.js/toastr.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/datatables/js/jquery.dataTables.js"></script>

    <script src="~/datatables/js/dataTables.bootstrap4.js"></script>
    <script src="~/js/bootbox.min.js"></script>
    <script src="~/typeahead.js/typeahead.bundle.js"></script>
    <script src="~/toastr.js/toastr.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
